package model;

public class Employee extends Person {
	private double salary;
	
	public Employee(String name, String address, String zip, String city, String phone, double salary) {
		super(name, address, zip, city, phone);
		setSalary(salary);
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		this.salary = salary;
	}

}
