package model;

public class Customer extends Person {
	private String type;
	
	public Customer(String name, String address, String zip, String city, String phone, String type) {
		super(name, address, zip, city, phone);
		setType(type);
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

}
